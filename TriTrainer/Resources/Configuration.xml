        </environment>
    </environments>
    <mappers>
          
        <mapper resource="es/tri/dao/sqlmap/TriTUsuariosMapper.xml"/>
        <mapper resource="es/tri/dao/sqlmap/TriTiEntrenoUsuarioMapper.xml"/>
        <mapper resource="es/tri/dao/sqlmap/TriTEntrenosMapper.xml"/>
